{
  "name": "mock-socket",
  "version": "0.8.2",
  "description": "A mocking library for WebSockets",
  "main": "./dist/mock-socket",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "./node_modules/broccoli-cli/bin/broccoli serve",
    "test": "npm run build; ./node_modules/testem/testem.js ci",
    "build": "rm -rf dist; ./node_modules/broccoli-cli/bin/broccoli build dist",
    "browser": "npm run build; ./node_modules/testem/testem.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thoov/mock-socket.git"
  },
  "keywords": [
    "websockets",
    "mock",
    "mocksocket",
    "sockets"
  ],
  "author": "Travis Hoover",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thoov/mock-socket/issues"
  },
  "homepage": "https://github.com/thoov/mock-socket",
  "devDependencies": {
    "URIjs": "^1.15.0",
    "broccoli": "^0.16.3",
    "broccoli-babel-transpiler": "^5.2.3",
    "broccoli-cli": "^1.0.0",
    "broccoli-concat": "0.0.13",
    "broccoli-fast-browserify": "^0.2.7",
    "broccoli-funnel": "^0.2.3",
    "broccoli-jshint": "^0.5.6",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-uglify-js": "^0.1.3",
    "del": "^0.1.3",
    "express": "^4.13.1",
    "qunitjs": "^1.18.0",
    "testem": "^0.6.23",
    "ws": "^0.7.2"
  }
}
