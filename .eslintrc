{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": ["error", 120],
    "class-methods-use-this": ["error", { "exceptMethods": ["stopPropagation", "stopImmediatePropagation"] }],
    "consistent-return": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    import/no-extraneous-dependencies: 0,
    "arrow-parens": 0
  }
}
